using System;


namespace URI
{
    class car
    {
        static void Main(string[] args)
        {
            int a, b, c;
            b = int.Parse(Console.ReadLine());
            a = int.Parse(Console.ReadLine());
            c = a + b;
            Console.WriteLine( "SOMA = {0}" , c);
            Console.ReadLine();
        }

    }

}
